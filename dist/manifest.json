{
  "manifest_version": 3,
  "name": "LabStar Links Extension",
  "version": "1.4",
  "description": "Add links to ScanPark inside LabStar",
  "permissions": [],
  "icons": {
    "64": "icon.png"
  },
  "content_scripts": [
    {
      "matches": [
        "https://lms.3shape.com/*",
        "https://oralart.labstar.com/*"
      ],
      "js": [
        "src/addScanToTable.js-loader.js",
        "src/addLastScanLinkToCasePopup.js-loader.js"
      ],
      "all_frames": true
    }
  ],
  "web_accessible_resources": [
    {
      "resources": [
        "icon.png"
      ],
      "matches": [
        "<all_urls>"
      ]
    },
    {
      "matches": [
        "<all_urls>"
      ],
      "resources": [
        "**/*",
        "*"
      ],
      "use_dynamic_url": false
    }
  ],
  "background": {
    "service_worker": "service-worker-loader.js",
    "type": "module"
  }
}
