{
    "manifest_version": 3,
    "name": "JsonTable",
    "version": "1.0",
    "description": "View JSON files in a table",
    "permissions": [],
    "icons": {
        "64": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "*://*/*/*.json",
                "file://*/*.json",
                "*://*/*/*.jsont",
                "file://*/*.jsont"
            ],
            "js": [
                "src/main.js"
            ],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icon.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}