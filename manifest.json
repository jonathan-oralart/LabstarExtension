{
    "manifest_version": 2,
    "name": "Labstar Links Extension",
    "version": "1.0",
    "description": "Add links to scanpark inside labstar",
    "author": "Jonathan de Wet",
    "permissions": [
        "activeTab"
    ],
    "browser_action": {
        "default_popup": "popup.html",
        "default_icon": {
            "16": "icon.png",
            "48": "icon.png",
            "128": "icon.png"
        }
    },
    "content_scripts": [
        {
            "matches": [
                "https://oralart.labstar.com/*"
            ],
            "js": [
                "content.js"
            ],
            "all_frames": true
        }
    ]
}