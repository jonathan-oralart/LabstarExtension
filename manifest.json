{
    "manifest_version": 3,
    "name": "Labstar Links Extension",
    "version": "1.0",
    "description": "Add links to scanpark inside Labstar",
    "permissions": [
        "activeTab"
    ],
    "icons": {
        "64": "icon.png"
    },
    "content_scripts": [
        {
            "matches": [
                "https://oralart.labstar.com/*"
            ],
            "js": [
                "content.js"
            ],
            "all_frames": true
        }
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "icon.png"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}